    "#Without Using Autograd\n",
    "import math\n",
    "def dz_dx(x):\n",
    "    return 2*x*math.cos(x**2)\n",
    "#Expressions\n",
    "# y=x**2\n",
    "# z=math.sin(y)\n",
    "dz_dx(3)"
