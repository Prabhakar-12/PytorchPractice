   "source": [
    "#Using Autograd\n",
    "import torch\n",
    "x=torch.tensor(3.0,requires_grad=True)\n",
    "y=x**2\n",
    "z=torch.sin(y)\n",
    "z.backward()\n",
    "x.grad"
   ],
